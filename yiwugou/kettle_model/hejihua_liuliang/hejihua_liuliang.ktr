    <connection>nginxraw</connection>
    <sql>SELECT COUNT(1) pv,COUNT(DISTINCT ip) ip,COUNT(DISTINCT session_id_year) uv,host_name,
	&apos;${YESTERDAY_STR}&apos; day_date
FROM nginx_access_log_raw
WHERE vist_time &gt;=   &apos;${YESTERDAY_STR}&apos;
AND  vist_time &lt; &apos;${DAY_STR}&apos;  
AND fileid = (SELECT id FROM apache_log_file
		WHERE fullname LIKE &apos;%hezuo%&apos;
		ORDER BY ts DESC LIMIT 0,1)
GROUP BY host_name
ORDER BY pv DESC </sql>
